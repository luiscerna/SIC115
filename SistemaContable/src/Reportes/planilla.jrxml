	</variable>
	<variable name="TotalDescuentos" class="java.lang.Double">
		<variableExpression><![CDATA[(($V{ISSS})+($V{AFP}))]]></variableExpression>
	</variable>
	<variable name="Neto" class="java.lang.Double">
